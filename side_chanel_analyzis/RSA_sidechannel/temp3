
  //SIGNING HASHED DATA function, in the next two loops.
  gnutls_datum_t low_hashed_data = rsa_data_sign(privkey, GNUTLS_DIG_SHA256, 0, &low_hamming_data, FALSE, FALSE, FALSE, NULL);
  output_file = fopen("LHWData_HashedDataSign_keyX.txt", "w");
  for(int i=0; i<NUMBER_OF_ITERATIONS; ++i){
    rsa_hashedata_sign(privkey, &low_hashed_data, GNUTLS_DIG_SHA256, 0, FALSE, FALSE, output_file); 
  };
  fclose(output_file);

  gnutls_datum_t high_hashed_data = rsa_data_sign(privkey, GNUTLS_DIG_SHA256, 0, &high_hamming_data, FALSE, FALSE, FALSE, NULL);
  output_file = fopen("HHWData_HashedDataSign_keyX.txt", "w");
  for(int i=0; i<NUMBER_OF_ITERATIONS; ++i){
    rsa_hashedata_sign(privkey, &high_hashed_data, GNUTLS_DIG_SHA256, 0, FALSE, FALSE, output_file);  
  };
  fclose(output_file);
  return 0;
}